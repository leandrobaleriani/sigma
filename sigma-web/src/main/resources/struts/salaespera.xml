<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>

	<package name="salaespera" extends="default" namespace="/salaespera">
		<action name="show" class="salaespera.action">
			<result name="success">/view/adm/monitor/sala_espera.jsp</result>
			<result name="json" type="json">
				<param name="root">jsonData</param>
			</result>
		</action>

		<action name="lugarAtencion" class="salaespera.action">
			<result name="success">/view/adm/monitor/seleccionarLugarAtencion.jsp</result>
			<result name="json" type="json">
				<param name="root">jsonData</param>
			</result>
		</action>
	</package>

</struts>